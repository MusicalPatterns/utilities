{
  "compilerOptions": {
    "module": "commonjs",
    "allowJs": true,
    "moduleResolution": "node",
    "target": "es5",
    "downlevelIteration": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "noImplicitReturns": true,
    "strict": true,
    "pretty": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  }
}
